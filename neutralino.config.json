{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/www/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*"],
  "extensions": [
    {
      "id": "js.neutralino.planify.generator",
      "command": "node ./extensions/generator.js"
    }
  ],
  "modes": {
    "window": {
      "title": "AI-Gantt",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/www/icon.png"
    }
  },
  "cli": {
    "binaryName": "AI-Gantt",
    "resourcesPath": "/www/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/www/neutralino.js",
    "binaryVersion": "6.2.0",
    "clientVersion": "6.2.0"
  }
}
